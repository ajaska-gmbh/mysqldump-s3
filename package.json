{
  "name": "mysqldump-s3",
  "version": "1.0.0",
  "description": "Node.js CLI tool to dump MySQL databases and upload to Amazon S3, with backup listing and restore functionality.",
  "main": "dist/index.js",
  "bin": {
    "mysqldump-s3": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajaska-gmbh/docker-mysqldump-s3.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ajaska-gmbh/docker-mysqldump-s3/issues"
  },
  "homepage": "https://github.com/ajaska-gmbh/docker-mysqldump-s3#readme",
  "keywords": [
    "mysql",
    "mysqldump",
    "s3",
    "backup",
    "restore",
    "cli",
    "aws"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "^3.828.0",
    "chalk": "^4.1.2",
    "cli-progress": "^3.12.0",
    "commander": "^14.0.0",
    "inquirer": "^8.2.6",
    "js-yaml": "^4.1.0",
    "mysql2": "^3.14.1"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/inquirer": "^8.2.10",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.0.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "jest": "^30.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
